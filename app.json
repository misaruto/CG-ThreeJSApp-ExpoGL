{
  "expo": {
    "name": "threejsapp",
    "slug": "threejsapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.jpg",
    "splash": {
      "image": "./assets/icon.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#5c9e32"
    },
    "permissions":[],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.misael.threeJsApp",
      "versionCode":0.1,
      "adaptiveIcon": {
        "backgroundColor": "#5c9e32"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
